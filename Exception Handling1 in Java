import java.util.*;

class Main{
    public static void main(String []args){
        try{
            System.out.println(25/5);
            System.out.println("Try block");
        }
        catch(Exception e){
            System.out.println("Catch block");
        }
        finally{
            //System.out.println(25/0);
            System.out.println("Finally block");
        }   
    }
}
