class Car
{
    int carNum;
    String carName;
    
    void getCarNum(int carNumber)
    {
        this.carNum = carNumber;
    }
    int setCarNum()
    {
        return this.carNum;
    }
    void getCarName(String carModel)
    {
        this.carName = carModel;
    }
    String setCarName()
    {
        return this.carName;
    }
}

public class Main
{
	public static void main(String[] args) 
	{
	    Car audi = new Car();
	    Car benz = new Car();
	    
	    audi.getCarNum(1234);
		benz.getCarNum(5678);
		
		audi.getCarName("AU15");
		benz.getCarName("BE19");
		
		System.out.println(audi.setCarNum());
		System.out.println(benz.setCarNum());
		
		System.out.println(audi.setCarName());
		System.out.println(benz.setCarName());
	}
}

